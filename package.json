{
  "name": "auria-maestro",
  "version": "0.0.2",
  "mode": "development",
  "description": "Auria Maestro - Build RESTful API's",
  "main": "dist/Maestro.js",
  "types": "dist/Maestro.d.ts",
  "author": "Nicholas Frai",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "jest": "^26.2.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fastify": "^3.6.0",
    "fastify-cookie": "^4.1.0",
    "fastify-helmet": "^5.0.3",
    "fastify-multipart": "^3.2.1",
    "tslib": "^2.0.0"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/__tests/(?!coverage/).*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "json"
    ]
  }
}